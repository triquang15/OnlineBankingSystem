<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="online_banking" match-name=".*"/>
  <table-filter match-catalog="online_banking" match-name="account"/>
  <table-filter match-catalog="online_banking" match-name="transactiondetails"/>
</hibernate-reverse-engineering>